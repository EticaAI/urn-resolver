{
  "_meta": {
    "Cache-Control": {
      "_cc_mode": "internal"
    },
    "spec": [
      "https://github.com/EticaAI/urn-resolver"
    ],
    "examples": [
      {
        "in.urn": "urn:resolver:ping",
        "out.[0].iri": "{{ urnresolver }}/_/resolver/ping.php"
      }
    ]
  },
  "rules": [
    {
      "in": {
        "urn": "urn:resolver:(.*)"
      },
      "internal": true
    },
    {
      "in": {
        "urn": "urn:resolver:ping"
      },
      "internal": true
    },
    {
      "in": {
        "urn": "urn:resolver:cron:run"
      },
      "internal": true
    }
  ]
}
