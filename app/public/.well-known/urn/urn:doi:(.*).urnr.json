{
  "$schema": "urn:resolver:schema:urnr",
  "meta": {
    "spec": "https://www.doi.org/doi_handbook/2_Numbering.html#2.6.3",
    "examples": [
      {
        "in.urn": "urn:doi:10.1000/182",
        "out.[0].iri": "https://doi.org/urn:doi:10.1000/182"
      }
    ]
  },
  "data": null,
  "rules": [
    {
      "in": {
        "urn": "urn:doi:(.*)"
      },
      "lb": true,
      "out": [
        {
          "iri": "https://doi.org/{{ in[0] }}",
          "lb_w": 100,
          "http_status": 302
        }
      ]
    }
  ]
}
