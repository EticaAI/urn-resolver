{
  "//comment": "Rename urnresolver.dist.conf.json to urnresolver.conf.json",
  "base_iri": "http://localhost:8000",
  "resolver_status_pages": ["https://stats.uptimerobot.com/z6Y43IGQkL"],
  "Cache-Control": {
    "default": {
      "_cc_prefix": "public",
      "max-age": 3600,
      "s-maxage": 600,
      "stale-while-revalidate": 600,
      "stale-if-error": 600
    },
    "default404": {
      "_cc_prefix": "public",
      "max-age": 900,
      "s-maxage": 60,
      "stale-while-revalidate": 900,
      "stale-if-error": 900
    },
    "default410": {
      "_cc_prefix": "public",
      "max-age": 3600,
      "s-maxage": 600,
      "stale-while-revalidate": 600,
      "stale-if-error": 600
    },
    "default501": {
      "_cc_prefix": "public",
      "max-age": 3600,
      "s-maxage": 600,
      "stale-while-revalidate": 600,
      "stale-if-error": 0
    },
    "critical": {
      "_cc_prefix": "public",
      "max-age": 30,
      "s-maxage": 30,
      "stale-while-revalidate": 10,
      "stale-if-error": 10
    },
    "internal": {
      "_cc_prefix": "public",
      "max-age": 0,
      "s-maxage": 900,
      "stale-while-revalidate": 0,
      "stale-if-error": 10
    }
  }
}